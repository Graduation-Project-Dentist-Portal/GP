@page
@model DentistPortal_Client.Pages.HomeModel
@{
    <a asp-page="/DoctorPages/MedicalCases/DisplayMedicalCases">Medical Cases</a>
    <a asp-page="/DoctorPages/MedicalCases/MyMedicalCases">My Cases</a>
}
@if (@Model.Msg != null)
{
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    if (@Model.Status == "success")
    {
        <script type="text/javascript">
            Swal.fire({
                icon: 'success',
                title: '@Model.Msg',
                showConfirmButton: false,
                timer: 1500
            })
        </script>
    }
}
